// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.db": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.map": true,
        "**/*?.js": {
            "when": "$(basename).tsx"
        }
    },
    "typescript.tsdk": "./node_modules/typescript/lib",
    "editor.trimAutoWhitespace": true
}